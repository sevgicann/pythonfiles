    "def counter_elements(*list):\n",
    " if not list:\n",
    "    raise ValueError(\"Error: List is empty \")\n",
    " else:\n",
    "    return len(list)\n",
    " \n",
    "try: \n",
    "  element_count=counter_elements(23,67,87,95,102)\n",
    "  print(f\"The list has {element_count} elements\")\n",
    "except ValueError as errorMessage:\n",
    "  print(errorMessage)"
